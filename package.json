{
  "name": "webpack-react-typescript-boilderplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "rm -rf dist/ && webpack --config webpack.prod",
    "test": "jest"
  },
  "author": {
    "name": "Clemens Paumgarten",
    "email": "clemenspaumgarten@gmail.com",
    "url": "http://clemens.rocks"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@svgr/webpack": "^4.3.3",
    "@types/jest": "^22.2.3",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^4.4.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^3.3.2",
    "enzyme": "^3.3.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^23.1.0",
    "jest-enzyme": "^6.0.1",
    "lint-staged": ">=8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^7.0.1",
    "styled-components": "^4.4.1",
    "terser-webpack-plugin": "^2.3.0",
    "tsconfig-paths-webpack-plugin": "^3.1.3",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.7.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "^3.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(js|tsx?)$",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePaths": [
      "src"
    ],
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
